{
  "name": "CrossTalkInfra",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy-sandbox": "cdk -a cdk.out/assembly-Sandbox deploy '*' --require-approval never",
    "lint": "eslint --ext .ts --ext .tsx --ext .js --ext .jsx ./src",
    "lint:fix": "eslint --ext .ts --ext .tsx --ext .js --ext .jsx --fix ./src"
  },
  "devDependencies": {
    "@types/node": "10.17.5",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "aws-cdk": "^1.87.1",
    "eslint": "7.6.0",
    "eslint-plugin-header": "3.0.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint:fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.87.1",
    "@aws-cdk/aws-appsync": "^1.87.1",
    "@aws-cdk/aws-cloudformation": "^1.87.1",
    "@aws-cdk/aws-cognito": "^1.87.1",
    "@aws-cdk/aws-iam": "^1.87.1",
    "@aws-cdk/aws-lambda": "^1.87.1",
    "@aws-cdk/aws-s3": "^1.87.1",
    "@aws-cdk/aws-s3-deployment": "^1.87.1",
    "@aws-cdk/core": "^1.87.1",
    "@aws-cdk/custom-resources": "^1.87.1",
    "aws-sdk": "2.731.0",
    "source-map-support": "0.5.19"
  }
}
