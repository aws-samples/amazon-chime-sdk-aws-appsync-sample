{
  "name": "CrossTalkInfra",
  "version": "0.1.0",
  "license": "MIT-0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy-sandbox": "cdk -a cdk.out/assembly-Sandbox deploy '*' --require-approval never",
    "lint": "eslint --ext .ts --ext .tsx --ext .js --ext .jsx ./src",
    "lint:fix": "eslint --ext .ts --ext .tsx --ext .js --ext .jsx --fix ./src"
  },
  "devDependencies": {
    "@types/node": "10.17.5",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "aws-cdk-lib": "^2.93.0",
    "eslint": "^7.32.0",
    "eslint-plugin-header": "3.0.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint:fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "aws-cdk-lib": "^2.93.0",
    "aws-sdk": "2.814.0",
    "constructs": "^10.0.0",
    "source-map-support": "0.5.19"
  },
  "resolutions": {
    "netmask": "^2.0.1",
    "ansi-regex": "^5.0.1",
    "minimatch": "^3.0.5",
    "xml2js": "^0.5.0"
  }
}
